  body.body-green
    .page-wrapper
      header#header.header
        .container
          .branding
            h1.logo
              a(href="index.html")
                span.icon_documents_alt.icon(aria-hidden="true")
                span.text-highlight Feedr
                span.text-bold App
      .doc-wrapper
        .container
          .doc-body
            .doc-content
              .doc-content-inner
                block content
            .doc-sidebar.hidden-xs
              nav#doc-nav
                ul#doc-menu.nav.doc-menu(data-spy="affix")
                  if sections
                    each section in sections
                      li
                        a.scrollto(href="#" + section + "-section")= section
