  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sdepold/feedrapp/issues"
  },
  "homepage": "https://github.com/sdepold/feedrapp",
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-import": "^2.2.0",
    "mocha": "^3.1.0",
    "sinon": "^1.12.2",
    "sinon-chai": "^2.7.0"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "bluebird": "^3.5.0",
    "body-parser": "~1.16.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.0",
