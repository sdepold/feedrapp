    "test": "yarn lint && yarn test:unit && yarn test:integration",
    "test:integration": "mocha test/integration/**/*.spec.js",
    "test:unit": "mocha test/unit/**/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sdepold/feedrapp.git"
  },
  "keywords": [
